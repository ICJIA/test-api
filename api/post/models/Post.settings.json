{
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "content": {
      "type": "text"
    },
    "category": {
      "model": "category"
    },
    "user": {
      "model": "user",
      "plugin": "users-permissions"
    },
    "tags": {
      "collection": "tag",
      "via": "posts",
      "dominant": true
    },
    "comments": {
      "collection": "comment",
      "via": "post"
    }
  }
}